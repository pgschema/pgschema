          "source": {
            "schema": "public",
            "name": "process_order",
            "definition": "\nDECLARE\n    total numeric;\nBEGIN\n    SELECT amount INTO total FROM orders WHERE id = order_id;\n    RETURN total - (total * discount_percent / 100);\nEND;\n",
            "return_type": "numeric",
